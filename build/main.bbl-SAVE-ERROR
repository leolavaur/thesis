    \entry{ring_Flowbasedbenchmarkdata_2017}{article}{}
      \name{author}{5}{}{%
        {{hash=e3dff8b07891530f7262730d0d5e78cb}{%
           family={Ring},
           familyi={R\bibinitperiod},
           given={Markus},
