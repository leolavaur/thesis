      \strng{namehash}{a9198b62c1075f5b75b10e7941988f56}
      \strng{fullhash}{6ebda5adadbd565448261e8d2b7acb04}
      \strng{bibnamehash}{a9198b62c1075f5b75b10e7941988f56}
      \strng{authorbibnamehash}{a9198b62c1075f5b75b10e7941988f56}
