      \strng{namehash}{447d73aca89c8db7d97e34486e67afe3}
      \strng{fullhash}{447d73aca89c8db7d97e34486e67afe3}
      \strng{bibnamehash}{447d73aca89c8db7d97e34486e67afe3}
      \strng{authorbibnamehash}{447d73aca89c8db7d97e34486e67afe3}
      \strng{authornamehash}{447d73aca89c8db7d97e34486e67afe3}
