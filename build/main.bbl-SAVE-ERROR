    \entry{chicco_advantagesMatthewscorrelation_2020}{article}{}
      \name{author}{2}{}{%
        {{hash=f3d4d09e12e591928a3b270c54528087}{%
           family={Chicco},
           familyi={C\bibinitperiod},
           given={Davide},
           giveni={D\bibinitperiod}}}%
        {{hash=e758b61322d8aaddcad00acf28472ea6}{%
