      \strng{namehash}{5c2bce96bd8b4d0e06d1868c73a374bf}
      \strng{fullhash}{d6a078bf2259659348d1117f75f30795}
      \strng{bibnamehash}{5c2bce96bd8b4d0e06d1868c73a374bf}
      \strng{authorbibnamehash}{5c2bce96bd8b4d0e06d1868c73a374bf}
